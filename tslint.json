{
	"defaultSeverity": "error",
	"extends": ["tslint:recommended"],
	"rules": {
		"quotemark": [true, "single"],
		"indent": [true, "tabs"],
		"member-access": false,
		"no-console": false,
		"ordered-imports": false,
		"interface-over-type-literal": false,
		"trailing-comma": [
			true,
			{
				"multiline": {
					"objects": "always",
					"arrays": "always",
					"functions": "never",
					"typeLiterals": "always"
				}
			}
		]
	},
	"rulesDirectory": []
}
